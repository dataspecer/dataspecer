{
  "compilerOptions": {
    "skipLibCheck": true,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "moduleResolution": "nodenext",
    "module": "NodeNext",
    "rewriteRelativeImportExtensions": true,
    "outDir": "./lib",
    "sourceMap": true,
    "target": "ESNext",
    "types": ["vitest/globals"]
  },
  "include": [
    "src/**/*", "src/tests/support/jsonLdCreator.ts", "node_modules/stream-chunks/chunks.js"
  ],
  "exclude": [
    "src/tests/","node_modules/*","src/tests"
  ]
}
